#include<iostream>
using namespace std;
struct person
{
	string name;
	int age;
	float salary;
};
int main()
{
	person per;
	cout << "please enter your name" << endl;
	cin >> per.name;
	cout << "please enter your age" << endl;
	cin >> per.age;
	cout << "please enter your salary" << endl;
	cin >> per.salary;
}
